<script is:inline>
  const root = document.querySelector(':root');
  const ringContainer = document.querySelector('#ringContainer');
  const eye1 = document.querySelector('#Eye-Left');
  const eye2 = document.querySelector('#Eye-Right');
  const eyesOpen = document.querySelector('#eyes');
  const eyesClosed = document.querySelector('#eyes-closed-happy');
  const eyebrows = document.querySelector('#eyebrows');

  function throttle(callback, interval) {
    let enableCall = true;
    return function (...args) {
      if (!enableCall) return;

      enableCall = false;
      callback.apply(this, args);
      setTimeout(() => (enableCall = true), interval);
    };
  }

  const xyCache = new Map();
  function getEyeXYPositions(evt) {
    const { pageX, pageY } = evt;
    const { innerWidth: w, innerHeight: h } = window;
    const keyX = `${pageX},${w}`,
      keyY = `${pageY},${h}`;
    let x, y;

    if (xyCache.has(keyX)) {
      x = xyCache.get(keyX);
    } else {
      x = Math.round((-(w / 2 - pageX) / 90) * 100) / 100;
      xyCache.set(keyX, x);
    }

    if (xyCache.has(keyY)) {
      y = xyCache.get(keyY);
    } else {
      y = Math.round((-(h / 2 - pageY) / 90) * 100) / 100;
      xyCache.set(keyY, y);
    }

    return { x, y };
  }

  const onMouseMove = (evt) => {
    const { x, y } = getEyeXYPositions(evt);
    eye1.style.transform = `translateY(${y}px) translateX(${x}px)`;
    eye2.style.transform = `translateY(${y}px) translateX(${x}px)`;
    root.style.setProperty('--eyebrow-y', `${Math.min(1, y)}px`);
  };

  window.addEventListener('mousemove', throttle(onMouseMove, 16));

  function onRingContainerHover(evt) {
    if (evt.target.nodeName === 'IMG') {
      if (evt.type === 'mouseover') {
        eyesOpen.classList.add('hidden');
        eyesClosed.classList.remove('hidden');
      }
      if (evt.type === 'mouseout') {
        eyesOpen.classList.remove('hidden');
        eyesClosed.classList.add('hidden');
      }
    }
  }
  ringContainer.onmouseover = ringContainer.onmouseout = onRingContainerHover;

  const avatarContainer = document.querySelector('#avatarContainer');

  function onAvatarContainerHover(evt) {
    if (evt.type === 'mouseover') {
      eyebrows.classList.add('eyebrows-raised');
      ringContainer.classList.add('animate-spin-once');
    }
    if (evt.type === 'mouseout') {
      eyebrows.classList.remove('eyebrows-raised');
      ringContainer.classList.remove('animate-spin-once');
    }
  }

  avatarContainer.onmouseover = avatarContainer.onmouseout =
    onAvatarContainerHover;
</script>

<style>
  :root {
    --eyebrow-y: -5px;
  }

  #avatarContainer > * #mouth-open {
    opacity: 0;
  }
  #avatarContainer > * #mouth-closed {
    opacity: 1;
  }
  #avatarContainer:hover > * #mouth-open {
    opacity: 1;
  }
  #avatarContainer:hover > * #mouth-closed {
    opacity: 0;
  }

  #eyebrows {
    transition: all 0.12s ease;
  }

  .eyebrows-follow {
    transform: translateY(var(--eyebrow-y));
  }

  .eyebrows-raised {
    transform: translateY(-5px) !important;
  }
</style>
